<style type="text/css">

.box_area{

	width:930px;
    margin:0 auto;


}

.boxes_column{
	float:left;
	width:310px;
	overflow:hidden;
}
.box_area_item{

        width:280px;
        padding:10px;
        margin:5px;

}
.box_area_item p{color:#333333;font-size:14px;font-family:Verdana;}
.box_area_item span{font-weight:bold;font-size:16px;}
.box_area_item .attachments{}
.box_area_item .attachments .item{display:inline-block;overflow:hidden;margin:5px;}
.box_area_item .attachments .item .full{display:none;}
.box_area_item .attachments .item h1{font-size:14px;color:#333333;margin:0px;padding:0px;}
.box_area_item .attachments .item p{font-size:12px;color:#222222;font-style:italic;}
.box_area_item .rate{font-size:16px;font-weight:bold;color:#55ff00;}
.box_area_clear{

	clear:both;

}

</style>

</head>
